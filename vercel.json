{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.fastmultimedia.site"
        }
      ],
      "destination": "https://fastmultimedia.site/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "fastmultimedia.site"
        },
        {
          "type": "header",
          "key": "x-forwarded-proto",
          "value": "http"
        }
      ],
      "destination": "https://fastmultimedia.site/:path*",
      "permanent": true
    }
  ]
}